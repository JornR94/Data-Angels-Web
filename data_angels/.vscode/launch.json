    "version": "0.2.0",
    "configurations": [
      {
        "name": "dataangels web",
        "request": "launch",
        "type": "dart",
        "flutterMode": "debug",
        "deviceId": "chrome",
        "args": [
          "--web-renderer",
          "canvaskit",
          "--web-hostname=localhost",
          "--web-port=4000",
        ],
      },
      {
        "name": "dataangels mobile",
        "request": "launch",
        "type": "dart",
        "flutterMode": "debug",
        "deviceId": "chrome",
        "args": [
          "--web-renderer",
          "canvaskit",
          "--web-hostname=192.168.1.254",
          "--web-port=4000",
        ],
      },
      {
        "name": "dataangels",
        "request": "launch",
        "type": "dart"
      },
      {
        "name": "dataangels (profile mode)",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile"
      },
    ]
}